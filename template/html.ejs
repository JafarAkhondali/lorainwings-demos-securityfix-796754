  <style>
    .title {
      width: 100%;
      margin-top: 20px;
      padding-left: 100px;
      color: #555;
    }

    .container {
      display: flex;
      flex-wrap: wrap;
      padding: 20px 100px;
    }

    .container .item{
      width: 20%;
      flex-basis: 20%;
      margin-bottom: 10px;
    }

    .item .icon {
      width: 18px;
      height: 18px;
      margin-right: 3px;
    }

    .item .h-link {
      display: flex;
      justify-content: flex-start;
      align-items: flex-start;
      color: #000;
      font-size: 14px;
    }

    .item .text {
      max-width: calc(100% - 18px);
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
  </style>
</head>

<body>
  <h1 class="title">DEMOS小案例预览列表</h1>
  <div class="container">
  <% for( let index=0; index < list.length; index++ ) { %>
    <% if (list[index].includes('html')) { %>
      <div class="item" title="<%= index %>、<%= list[index] %>">
        <a href="https://lorainwings.github.io/demos/<%= list[index] %>" class="h-link">
          <svg class="icon" viewBox="0 0 1000 1000">
            <use xlink:href="#html-icon"></use>
          </svg>
          <span class="text"><%= index %>、<%= list[index] %></span>
        </a>
      </div>
    <% } %>
  <% } %>
  </div>
</body>

</html>
